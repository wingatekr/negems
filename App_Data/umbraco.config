    <BlogPostRepository id="1075" key="3e5030d3-0737-4844-a5bb-b300d4c939d4" parentID="1063" level="2" creatorID="0" sortOrder="2" createDate="2017-01-29T21:16:15" updateDate="2017-03-11T12:06:55" nodeName="Blog" urlName="blog" path="-1,1063,1075" isDoc="" nodeType="1059" creatorName="karissa" writerName="karissa" writerID="0" template="1054" nodeTypeAlias="BlogPostRepository">
